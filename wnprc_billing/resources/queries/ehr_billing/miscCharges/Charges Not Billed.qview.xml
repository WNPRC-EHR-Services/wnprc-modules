<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="date"/>
        <column name="project"/>
        <column name="investigator"/>
        <column name="chargeId/name">
            <properties>
                <property name="columnTitle" value="Charge Item"/>
            </properties>
        </column>
        <column name="chargeId/chargeCategoryId/name">
            <properties>
                <property name="columnTitle" value="Category"/>
            </properties>
        </column>
        <column name="chargeGroup"/>
        <column name="chargetype"/>
        <column name="debitedaccount"/>
        <column name="creditedaccount"/>
        <column name="quantity"/>
        <column name="unitCost"/>
        <column name="comment"/>
        <column name="invoiceId"/>
    </columns>
    <filters>
        <filter column="invoiceId" operator="isblank" value="true"/>
    </filters>
    <sorts>
        <sort column="date" descending="true" />
    </sorts>
</customView>